<div id="container-new-review" style="color:white">
<h1>Add your review</h1>
<%= simple_form_for [@foodtruck, @review] do |f| %>
  <%= f.input :content %>
  <%= f.input :rating, :as => :select, :collection => Review::RATING, include_blank: false %>
  <%= f.submit "Submit", class: "btn btn-primary " %>
<% end %>
</div>
